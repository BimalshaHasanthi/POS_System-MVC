<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LogOutConfermation2FormController">
   <children>
      <AnchorPane fx:id="logoutContext" layoutX="7.0" prefHeight="1080.0" prefWidth="1920.0">
         <children>
            <AnchorPane layoutX="659.0" layoutY="330.0" prefHeight="401.0" prefWidth="543.0" style="-fx-background-color: white;">
               <children>
                  <Label layoutX="44.0" layoutY="208.0" text="Do you want to exit ? ">
                     <font>
                        <Font name="Montserrat Medium" size="30.0" />
                     </font>
                  </Label>
                  <JFXButton buttonType="RAISED" layoutX="209.0" layoutY="273.0" onAction="#logOutOnAction" prefHeight="58.0" prefWidth="125.0" style="-fx-background-color: #4241b9; -fx-background-radius: 50;" text="Yes" textFill="WHITE">
                     <font>
                        <Font name="Montserrat Medium" size="30.0" />
                     </font>
                  </JFXButton>
                  <JFXButton buttonType="RAISED" layoutX="356.0" layoutY="273.0" onAction="#cancelLogoutOnAction" prefHeight="58.0" prefWidth="125.0" style="-fx-background-color: #4142b9; -fx-background-radius: 50;" text="No" textFill="WHITE">
                     <font>
                        <Font name="Montserrat Medium" size="30.0" />
                     </font>
                  </JFXButton>
                  <ImageView fitHeight="172.0" fitWidth="331.0" layoutX="106.0" layoutY="34.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../assets/logo2.png" />
                     </image>
                  </ImageView>
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
